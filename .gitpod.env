# -------------------------------
# Spring Boot Database (Supabase)
# -------------------------------
SPRING_DATASOURCE_URL=jdbc:postgresql://<SUPABASE_HOST>:5432/<DB_NAME>?sslmode=require
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=<SUPABASE_PASSWORD>

# -------------------------------
# JWT Secret for Authentication
# -------------------------------
JWT_SECRET=very-strong-secret-please-change

# -------------------------------
# Optional Redis Configuration
# -------------------------------
REDIS_HOST=<REDIS_HOST>
REDIS_PORT=<REDIS_PORT>
REDIS_PASSWORD=<REDIS_PASSWORD>

# -------------------------------
# Server Port (optional, default 8080)
# -------------------------------
SERVER_PORT=8080
